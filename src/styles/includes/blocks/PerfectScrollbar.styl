.MapObjects
  &.ps > .ps__rail-x,
  &.ps > .ps__rail-y
    opacity 0.6

  .ps__thumb-x
    bottom 11px
    height 1px
    border-radius 0
    background-color $brown

  .ps__rail-x
    height 1px

  .ps--active-x > .ps__rail-x
    background $sandy

.Tabs_list
  &.ps > .ps__rail-x,
  &.ps > .ps__rail-y
    opacity 0.6
  
  .ps__thumb-x
    height 4px
    background $sandy
  
  .ps__rail-x
    height 4px