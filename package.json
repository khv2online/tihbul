  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build",
    "dev": "nodemon --watch gulpfile.js --watch package.json --ext js,json,styl,pug --exec gulp dev",
    "format": "gulp format",
    "validate": "gulp validate",
    "imagemin": "gulp imagemin"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:khv2online/frontend_builder_boilerplate.git"
  },
